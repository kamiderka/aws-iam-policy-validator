{
  "PolicyName": "AllowDynamoDB",
  "PolicyDocument": {
    "Version": "2008-10-17",
    "Statement": [
      {
        "Sid": "1",
        "Effect": "Allow",
        "Action": ["dynamodb:GetObject", "s3:GetObject"],
        "Resource": "arn:partition:service:region:account-id:resource-id"
      }
    ]
  }
}
