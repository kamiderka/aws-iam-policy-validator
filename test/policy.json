{
    "PolicyName": "AllowDynamoDB",
    "PolicyDocument": {
      "Version": "2012-10-17",
      "Statement": [
        {
          "Sid": "1",
          "Effect": "Allow",
          "Action": ["dynamodb:GetObject"],
          "Resource": "arn:aws:dynamodb:us-east-1:123456789012:table/*"
        }
      ]
    }
  }
  